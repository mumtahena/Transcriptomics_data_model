<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:include schemaLocation="HGNCSymbols.xsd"/>
    <xs:element name="tumorGeneExpressionTest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="author">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="firstName" type="xs:string"/>
                            <xs:element name="lastName" type="xs:string"/>
                            <xs:element minOccurs="1" name="id" type="xs:ID"/>
                            <xs:element name="authoringDateandTime" type="xs:dateTime"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="patient">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="firstName" type="xs:string"/>
                            <xs:element name="lastName" type="xs:string"/>
                            <xs:element minOccurs="0" name="patientId" type="xs:ID"/>
                            <xs:element name="dateOfBirth" type="xs:date"/>
                            <xs:element name="gender">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="Male|Female|Undifferentiated"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="orderingClinician">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="firstName" type="xs:string"/>
                            <xs:element name="lastName" type="xs:string"/>
                            <xs:element name="clinicianId" type="xs:ID"/>
                            <xs:element name="speciality" type="xs:string"/>
                            <xs:element name="orderingDate" type="xs:dateTime"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="reasonForTesting" type="xs:string"/>
                <xs:element minOccurs="0" name="relevantDiagnosis" type="xs:string"/>
                <xs:element name="sample">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="anatomicLocation" type="xs:string"/>
                            <xs:element name="analyte" type="xs:string"/>
                            <xs:element name="samplingDateTime" type="xs:dateTime"/>
                            <xs:element name="receivedDate" type="xs:dateTime"/>
                            <xs:element minOccurs="0" name="tumorSize" type="xs:long"/>
                            <xs:element minOccurs="0" name="nodeStatus">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string"/>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element minOccurs="0" name="notes" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="test">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="platform">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="platformName" type="xs:string"/>
                                        <xs:element name="platformVersion" type="xs:string"/>
                                        <xs:element minOccurs="0" name="sequencingLocation"
                                            type="xs:string"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="analysis">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element default="Absolute" name="typeOfAnalysis">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                  <xs:pattern value="Absolute|Differential"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="rawDataReference" type="xs:anyURI"/>
                                        <xs:element name="analysisMethod">
                                            <xs:complexType>
                                                <xs:choice>
                                                  <xs:element minOccurs="0" name="analysisPipeline">
                                                  <xs:complexType>
                                                  <xs:sequence>
                                                  <xs:element name="pipelineName" type="xs:string"/>
                                                  <xs:element name="pipelineVersion"
                                                  type="xs:string"/>
                                                  </xs:sequence>
                                                  </xs:complexType>
                                                  </xs:element>
                                                  <xs:element maxOccurs="unbounded"
                                                  name="analysisTool" minOccurs="0">
                                                  <xs:complexType>
                                                  <xs:sequence>
                                                  <xs:element name="toolName" type="xs:string"/>
                                                  <xs:element name="toolVersion" type="xs:string"/>
                                                  </xs:sequence>
                                                  </xs:complexType>
                                                  </xs:element>
                                                </xs:choice>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="referenceGenome">
                                            <xs:complexType>
                                                <xs:all>
                                                  <xs:element name="name" type="xs:string"> </xs:element>
                                                  <xs:element name="version" type="xs:string"/>
                                                  <xs:element name="geneAnnotationName"
                                                  type="xs:string"> </xs:element>
                                                  <xs:element name="geneAnnotationVersion"
                                                  type="xs:string"/>
                                                </xs:all>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="analyzedBy">
                                            <xs:complexType>
                                                <xs:sequence>
                                                  <xs:element name="firstName" type="xs:string"/>
                                                  <xs:element name="lastName" type="xs:string"/>
                                                  <xs:element minOccurs="1" name="id"
                                                  type="xs:string"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="analysisDateTime" type="xs:dateTime"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="results">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element maxOccurs="unbounded" minOccurs="1" name="gene">
                                            <xs:complexType>
                                                <xs:sequence>
                                                  <xs:element minOccurs="1" name="geneId">
                                                  <xs:complexType>
                                                  <xs:all maxOccurs="1">
                                                  <xs:element maxOccurs="1" minOccurs="0"
                                                  name="HGNCSymbol" type="HGNCSympols"> </xs:element>
                                                  <xs:element minOccurs="0" name="HGNCID"
                                                  type="xs:nonNegativeInteger"/>
                                                  <xs:element minOccurs="0" name="ENTREZID"
                                                  type="xs:int"/>
                                                  <xs:element minOccurs="0" name="ENSEMBLID"
                                                  type="xs:string"> </xs:element>
                                                  </xs:all>
                                                  </xs:complexType>
                                                  </xs:element>
                                                  <xs:element maxOccurs="1" minOccurs="1"
                                                  name="normalizedExpression">
                                                  <xs:complexType>
                                                  <xs:sequence>
                                                  <xs:element minOccurs="1" name="Value"
                                                  type="xs:double"> </xs:element>
                                                  <xs:element minOccurs="0" name="logBase"
                                                  type="xs:integer"> </xs:element>
                                                  </xs:sequence>
                                                  </xs:complexType>
                                                  </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="numberOfGenesReported" type="xs:integer"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="note" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
